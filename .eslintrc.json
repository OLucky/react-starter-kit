{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "sourceType": "module"
  },
  "plugins": [
    "react-hooks"
  ],
  "rules": {
    "no-delete-var":"error",
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn",
    "quotes": [
        "warn",            
        "single"
    ],
    "indent": ["error", 2],
    "semi":"warn",
    "no-mixed-spaces-and-tabs":"warn",
    "react-hooks/rules-of-hooks": "error"
},
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ]
}